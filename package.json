    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
